
public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
      int num1=100;
      int num2=200;
      
      int min=(num1 < num2)? num1:num2;
      int max=(num1 > num2)? num1:num2;
      
      System.out.println("Min value is "+min);
      System.out.println("Max value is "+max);
      
	}

}
