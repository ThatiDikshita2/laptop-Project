package oops;

class Parent{
	public void study() {
		System.out.println("Medicine");
	}
	public void property() {
		System.out.println("100Arc + !00Kgs of Gold");
    }
	public void marry() {
		System.out.println("Arrange Married");
	}	
}
class child extends Parent{
	// Overriding
	public void  marry() {
		System.out.println("Love Marriage");
	}
	public void study() {
		System.out.println("Medicine");
  }
}
	public class OverridingDemo {

	public static void main(String[] args) {
	  Parent p = new Parent();
	   p.property();
	   p.marry();
	   p.study();

	}
}

